<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.g4.shutu.t1.been.Userinfo" table="t_userinfo">
		<id name="uid" column="uid" type="integer">
			<generator class="foreign">
				<param name="property">emp</param>
			</generator>
		</id>
		<property name="uname" column="uname" type="string"></property>
		<property name="upwd" column="upwd" type="string"></property>
		<one-to-one name="emp" class="com.qhit.lh.g4.jack.t2.bean.Emp" cascade="all"></one-to-one>
	</class>
	
</hibernate-mapping>
